{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "LogstashListBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::cse-elk.test-backup"
            ]
        },
        {
            "Sid": "LogstashS3permissions",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::cse-elk.test-backup/*"
            ]
        }
    ]
}